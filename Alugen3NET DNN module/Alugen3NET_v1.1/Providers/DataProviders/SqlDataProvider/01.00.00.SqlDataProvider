	[CodeID],
	[CreatedDate],
	[CreatedByUser],
	[PortalID],
	[ModuleID],
	[CustomerID],
	[ProductID],
	[CodeUserName],
	[CodeInstalationCode],
	[CodeActivationCode],
	[CodeLicenseType],
	[CodeExpireDate],
	[CodeDays],
	[CodeRegisteredLevel]
FROM  {objectQualifier}AlugenCode
WHERE
	[CodeID] = @CodeID
GO
