	[CreatedDate],
	[CreatedByUser],
	[PortalID],
	[ModuleID],
	[CustomerID],
	[ProductID],
	[CodeUserName],
	[CodeInstalationCode],
	[CodeActivationCode],
	[CodeLicenseType],
	[CodeExpireDate],
	[CodeDays],
	[CodeRegisteredLevel]
FROM  {objectQualifier}AlugenCode

WHERE
	[PortalID] = @portalID AND
	[ModuleID] = @ModuleID AND
	[CustomerID] = @CustomerID
GO
