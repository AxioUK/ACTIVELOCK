CREATE PROCEDURE {databaseOwner}{objectQualifier}AlugenProductsAdd
	@PortalID int,
	@ModuleID int,
	@UserName nvarchar(100),
	@ProductName varchar(80),
	@ProductVersion varchar(20),
	@ProductVCode varchar(300),
	@ProductGCode varchar(600),
	@ProductPrice decimal(18, 6)
AS

INSERT INTO  {objectQualifier}AlugenProducts (
	CreatedByUser,
	CreatedDate,
	[PortalID],
	[ModuleID],
	[ProductName],
	[ProductVersion],
	[ProductVCode],
	[ProductGCode],
	[ProductPrice]
) VALUES (
	@UserName,
	getdate(),
	@PortalID,
	@ModuleID,
	@ProductName,
	@ProductVersion,
	@ProductVCode,
	@ProductGCode,
	@ProductPrice
)

select SCOPE_IDENTITY()
GO
SET QUOTED_IDENTIFIER OFF 
GO
SET ANSI_NULLS ON 
GO

SET QUOTED_IDENTIFIER OFF 
GO
SET ANSI_NULLS OFF 
GO

